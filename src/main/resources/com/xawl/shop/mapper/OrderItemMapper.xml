<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.xawl.shop.OrderItemMapper">

	<insert id="insert" parameterType="com.xawl.shop.domain.OrderItem">
		insert into
		db_orderitem(gid,gname,gprice,gimage,oid)
		values(
		#{gid},
		#{gname},
		#{gprice},
		#{gimage},
		#{oid}
		)
	</insert>

</mapper>